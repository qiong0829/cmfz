<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "../mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.cmfz.dao.LogDao">

    <insert id="insertLog">
        insert into log values(#{log.logId},#{log.logUser},#{log.logDatetime},
        #{log.logSourceStyle},#{log.logAction},#{log.logDetails},#{log.logResult})
    </insert>


</mapper>
