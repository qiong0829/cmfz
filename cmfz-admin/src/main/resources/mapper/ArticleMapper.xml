<mapper namespace="com.baizhi.cmfz.dao.ArticleDao">

    <insert id="insertArticle">
        insert into article values(#{article.articleId},#{article.articleName},
        #{article.guruId},#{article.articleBlurb},#{article.articlePushtime},
        #{article.articleStatus})
    </insert>


</mapper>
